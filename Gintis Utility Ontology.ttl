###  http://www.semanticweb.org/mdebe/ontologies/2024/7/util/OrState
:OrState rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSubProposition ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :State
                             ] ;
         rdfs:subClassOf :State ;
         <http://purl.org/dc/terms/contributor> <https://orcid.org/0000-0002-8824-9577> ;
         <http://purl.org/dc/terms/created> "2024-08-02T18:10:03Z"^^xsd:dateTime ;
         rdfs:comment "A state where only one of the sub-states will be the actual result. Each sub-state must have a probability and the probabilities must sum to 1." .


