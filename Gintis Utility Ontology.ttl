                          ] ;
      rdfs:subClassOf :Event ;
      <http://purl.org/dc/terms/contributor> <https://orcid.org/0000-0002-8824-9577> ;
      <http://purl.org/dc/terms/created> "2024-08-02T18:35:57Z"^^xsd:dateTime .


